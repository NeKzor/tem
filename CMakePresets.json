{
    "version": 8,
    "configurePresets": [
        {
            "name": "debug",
            "displayName": "debug",
            "description": "Using compilers: C = /usr/bin/i686-w64-mingw32-gcc, CXX = /usr/bin/i686-w64-mingw32-g++",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/build/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/i686-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/i686-w64-mingw32-g++",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "release",
            "displayName": "release",
            "description": "Using compilers: C = /usr/bin/i686-w64-mingw32-gcc, CXX = /usr/bin/i686-w64-mingw32-g++",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/build/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/i686-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/i686-w64-mingw32-g++",
                "CMAKE_BUILD_TYPE": "Release"
            }
        }
    ]
}